@article{panEffectsThresholdChoice2005,
  title = {Effects of Threshold Choice on Biological Conclusions Reached during Analysis of Gene Expression by {{DNA}} Microarrays},
  author = {Pan, Kuang-Hung and Lih, Chih-Jian and Cohen, Stanley N.},
