  langid = {english},
  keywords = {Disease genetics,Genetic variation,Rare variants,Risk factors},
  file = {/Users/karltayeb/Zotero/storage/L7TH7WFQ/Witte_Visscher_2014_The_contribution_of_genetic_variants_to_disease_depends_on_the_ruler.pdf}
