  langid = {english},
  keywords = {Gene regulation,Quantitative trait loci,Statistical methods,T cells,Transcriptomics},
  file = {/Users/karltayeb/Zotero/storage/MHWEH9U9/Nathan_Asgari_2022_Single-cell_eQTL_models_reveal_dynamic_T_cell_state_dependence_of_disease_loci.pdf;/Users/karltayeb/Zotero/storage/Q5JTDP2K/s41586-022-04713-1.html}
