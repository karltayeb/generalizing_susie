  title = {Gene-Set Analysis Is Severely Biased When Applied to Genome-Wide Methylation Data},
  author = {Geeleher, Paul and Hartnett, Lori and Egan, Laurance J. and Golden, Aaron and Raja Ali, Raja Affendi and Seoighe, Cathal},
