@article{witteContributionGeneticVariants2014,
  title = {The Contribution of Genetic Variants to Disease Depends on the Ruler},
  author = {Witte, John S. and Visscher, Peter M. and Wray, Naomi R.},
  date = {2014-11},
  journaltitle = {Nature Reviews Genetics},
  shortjournal = {Nat Rev Genet},
