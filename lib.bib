@misc{wangFalseDiscoveryRate2021,
  title = {False Discovery Rate Control with E-Values},
  author = {Wang, Ruodu and Ramdas, Aaditya},
  date = {2021-12-14},
  eprint = {2009.02824},
  eprinttype = {arxiv},
  primaryclass = {math, stat},
