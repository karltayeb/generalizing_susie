  author = {Eldjarn, Grimur Hjorleifsson and Ferkingstad, Egil and Lund, Sigrun H. and Helgason, Hannes and Magnusson, Olafur Th and Gunnarsdottir, Kristbjorg and Olafsdottir, Thorunn A. and Halldorsson, Bjarni V. and Olason, Pall I. and Zink, Florian and Gudjonsson, Sigurjon A. and Sveinbjornsson, Gardar and Magnusson, Magnus I. and Helgason, Agnar and Oddsson, Asmundur and Halldorsson, Gisli H. and Magnusson, Magnus K. and Saevarsdottir, Saedis and Eiriksdottir, Thjodbjorg and Masson, Gisli and Stefansson, Hreinn and Jonsdottir, Ingileif and Holm, Hilma and Rafnar, Thorunn and Melsted, Pall and Saemundsdottir, Jona and Norddahl, Gudmundur L. and Thorleifsson, Gudmar and Ulfarsson, Magnus O. and Gudbjartsson, Daniel F. and Thorsteinsdottir, Unnur and Sulem, Patrick and Stefansson, Kari},
  date = {2023-10},
  journaltitle = {Nature},
  volume = {622},
  number = {7982},
  pages = {348--358},
  publisher = {{Nature Publishing Group}},
  issn = {1476-4687},
  doi = {10.1038/s41586-023-06563-x},
  url = {https://www.nature.com/articles/s41586-023-06563-x},
  urldate = {2023-11-27},
  abstract = {High-throughput proteomics platforms measuring thousands of proteins in plasma combined with genomic and phenotypic information have the power to bridge the gap between the genome and diseases. Here we performed association studies of Olink Explore 3072 data generated by the UK Biobank Pharma Proteomics Project1 on plasma samples from more than 50,000 UK Biobank participants with phenotypic and genotypic data, stratifying on British or Irish, African and South Asian ancestries. We compared the results with those of a SomaScan v4 study on plasma from 36,000 Icelandic people2, for 1,514 of whom Olink data were also available. We found modest correlation between the two platforms. Although cis protein quantitative trait loci were detected for a similar absolute number of assays on the two platforms (2,101 on Olink versus 2,120 on SomaScan), the proportion of assays with such supporting evidence for assay performance was higher on the Olink platform (72\% versus 43\%). A considerable number of proteins had genomic associations that differed between the platforms. We provide examples where differences between platforms may influence conclusions drawn from the integration of protein levels with the study of diseases. We demonstrate how leveraging the diverse ancestries of participants in the UK Biobank helps to detect novel associations and refine genomic location. Our results show the value of the information provided by the two most commonly used high-throughput proteomics platforms and demonstrate the differences between them that at times provides useful complementarity.},
  issue = {7982},
  langid = {english},
  keywords = {Genome-wide association studies,Population genetics,Proteomic analysis,Proteomics,Quantitative trait},
  file = {/Users/karltayeb/Zotero/storage/VZSNYAME/Eldjarn_Ferkingstad_2023_Large-scale_plasma_proteomics_comparisons_through_genetics_and_disease.pdf}
}

@article{elorbanySinglecellSequencingReveals2022,
  title = {Single-Cell Sequencing Reveals Lineage-Specific Dynamic Genetic Regulation of Gene Expression during Human Cardiomyocyte Differentiation},
  author = {Elorbany, Reem and Popp, Joshua M. and Rhodes, Katherine and Strober, Benjamin J. and Barr, Kenneth and Qi, Guanghao and Gilad, Yoav and Battle, Alexis},
  date = {2022-01-21},
  journaltitle = {PLOS Genetics},
  shortjournal = {PLOS Genetics},
  volume = {18},
  number = {1},
  pages = {e1009666},
  publisher = {{Public Library of Science}},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1009666},
  url = {https://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1009666},
  urldate = {2022-04-14},
  abstract = {Dynamic and temporally specific gene regulatory changes may underlie unexplained genetic associations with complex disease. During a dynamic process such as cellular differentiation, the overall cell type composition of a tissue (or an in vitro culture) and the gene regulatory profile of each cell can both experience significant changes over time. To identify these dynamic effects in high resolution, we collected single-cell RNA-sequencing data over a differentiation time course from induced pluripotent stem cells to cardiomyocytes, sampled at 7 unique time points in 19 human cell lines. We employed a flexible approach to map dynamic eQTLs whose effects vary significantly over the course of bifurcating differentiation trajectories, including many whose effects are specific to one of these two lineages. Our study design allowed us to distinguish true dynamic eQTLs affecting a specific cell lineage from expression changes driven by potentially non-genetic differences between cell lines such as cell composition. Additionally, we used the cell type profiles learned from single-cell data to deconvolve and re-analyze data from matched bulk RNA-seq samples. Using this approach, we were able to identify a large number of novel dynamic eQTLs in single cell data while also attributing dynamic effects in bulk to a particular lineage. Overall, we found that using single cell data to uncover dynamic eQTLs can provide new insight into the gene regulatory changes that occur among heterogeneous cell types during cardiomyocyte differentiation.},
  langid = {english},
  keywords = {_tablet,Cardiomyocytes,Cell differentiation,Fibroblasts,Gene expression,Gene regulation,Genetics,Induced pluripotent stem cells,Nonlinear dynamics},
  file = {/Users/karltayeb/Zotero/storage/3CEKNB87/Elorbany_Popp_2022_Single-cell_sequencing_reveals_lineage-specific_dynamic_genetic_regulation_of.pdf;/Users/karltayeb/Zotero/storage/ITYMMW4N/article.html}
}

