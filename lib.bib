  title = {Identifying Disease-Critical Cell Types and Cellular Processes across the Human Body by Integration of Single-Cell Profiles and Human Genetics},
  author = {Jagadeesh, Karthik A. and Dey, Kushal K. and Montoro, Daniel T. and Mohan, Rahul and Gazal, Steven and Engreitz, Jesse M. and Xavier, Ramnik J. and Price, Alkes L. and Regev, Aviv},
