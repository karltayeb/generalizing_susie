  langid = {english},
  file = {/Users/karltayeb/Zotero/storage/DFG6SSXG/Allison et al. - 2006 - Microarray data analysis from disarray to consoli.pdf;/Users/karltayeb/Zotero/storage/9MSZJF2T/nrg1749.html}
