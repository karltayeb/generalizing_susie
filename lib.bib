  author = {Cuomo, Anna S. E. and Seaton, Daniel D. and McCarthy, Davis J. and Martinez, Iker and Bonder, Marc Jan and Garcia-Bernardo, Jose and Amatya, Shradha and Madrigal, Pedro and Isaacson, Abigail and Buettner, Florian and Knights, Andrew and Natarajan, Kedar Nath and Vallier, Ludovic and Marioni, John C. and Chhatriwala, Mariya and Stegle, Oliver},
  date = {2020-02-10},
  journaltitle = {Nature Communications},
