@article{penaPOWERENHANCEDMULTIPLEDECISION2011,
  title = {{{POWER-ENHANCED MULTIPLE DECISION FUNCTIONS CONTROLLING FAMILY-WISE ERROR AND FALSE DISCOVERY RATES}}},
  author = {Peña, Edsel A. and Habiger, Joshua D. and Wu, Wensong},
