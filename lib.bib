@article{zhouUNIFIEDFRAMEWORKVARIANCE2017,
  title = {A {{UNIFIED FRAMEWORK FOR VARIANCE COMPONENT ESTIMATION WITH SUMMARY STATISTICS IN GENOME-WIDE ASSOCIATION STUDIES}}},
  author = {Zhou, Xiang},
  date = {2017-12},
  journaltitle = {The annals of applied statistics},
  shortjournal = {Ann Appl Stat},
  volume = {11},
  number = {4},
